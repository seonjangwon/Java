package day07_20211013;

import java.util.Scanner;
import java.util.StringTokenizer;

public class Ex00_BjA {

	public static void main(String[] args) {
		// 백준 알고리즘 최댓값 최솟값구하기
		Scanner scan = new Scanner(System.in);
//		StringTokenizer st= new StringTokenizer(System.in);
		
		int n = 0; // 몇 개를 줄건가
		int b[] = new int[2];// 여기에 최솟값 최댓값을 넣을 거야
		int max = 0; // max는 무슨 숫자가 들어가도 상관없지만
		int min = 10000000; // min은 큰 숫자를 넣거나해야하니 제일 큰 숫자를 넣자
		
		n= scan.nextInt();
		int aSize = n;
		
		// 배열을 어떻게 받는거지
		int[] a = new int[n];
		// 스트링으로 받아서 스트링토큰으로 나눠서 for문으로 n개까지 배열입력을 하자
		String var = "";
		var = scan.nextLine();
		
				
				
	
		for (int i = 0; i < n; i++) {
			if (a[i] < min) {
				min = a[i];
			}
		}

		for (int i = 0; i < n; i++) {
			if (a[i] > max) {
				max = a[i];
			}
		}
		b[1] = max;
		b[0] = min;

		System.out.println(b[0]);
		System.out.println(b[1]);
	}

}
